set include BOOL off .

set include TRUTH-VALUE on .

fmod SIMPLE-NAT is
  sort Nat .
  op zero : -> Nat [ctor] .
  op s_ : Nat -> Nat [ctor] .
  op _+_ : Nat Nat -> Nat .
  vars N M : Nat .
  eq zero + N = N .
  eq s N + M = s (N + M) .
endfm